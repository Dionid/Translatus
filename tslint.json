{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "semicolon": [true, "never"],
    "max-line-length": {
      "options": [120]
    },
    "no-console": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "member-ordering": false
  },
  "rulesDirectory": []
}